@incollection{basanow3DSpatialData2008,
  langid = {english},
  title = {Towards {{3D Spatial Data Infrastructures}} ({{3D}}-{{SDI}}) Based on Open Standards — Experiences, Results and Future Issues},
  isbn = {978-3-540-72134-5 978-3-540-72135-2},
