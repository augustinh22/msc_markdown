  file = {C:\\Users\\b1041827\\Zotero\\storage\\BERWZTP2\\Holub and Fuchs - 2009 - Mitigating mountain hazards in Austria – legislati.pdf;C:\\Users\\b1041827\\Zotero\\storage\\KPVHB2K8\\2009.html}
}

