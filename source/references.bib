@article{giulianiBuildingEarthObservations2017,
  title = {Building an {{Earth Observations Data Cube}}: Lessons Learned from the {{Swiss Data Cube}} ({{SDC}}) on Generating {{Analysis Ready Data}} ({{ARD}})},
