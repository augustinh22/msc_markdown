@article{baraldiOperationalAutomaticRemote2012a,
  langid = {english},
  title = {Operational {{Automatic Remote Sensing Image Understanding Systems}}: {{Beyond Geographic Object}}-{{Based}} and {{Object}}-{{Oriented Image Analysis}} ({{GEOBIA}}/{{GEOOIA}}). {{Part}} 1: {{Introduction}}},
  volume = {4},
