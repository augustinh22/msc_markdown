  file = {C:\\Users\\b1041827\\Zotero\\storage\\KZV57385\\1-s2.0-S0168169910001638-main.pdf;C:\\Users\\b1041827\\Zotero\\storage\\3DHE9J3U\\S0168169910001638.html}
}

@article{shenSpatiotemporalAutocorrelationMeasures2016a,
