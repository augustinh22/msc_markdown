  keywords = {Fluvial   processes monitoring,Fluvial processes monitoring,Point-cloud data,Small catchment,Svalbard,Terrestrial laser scanning (TLS),airborne,dsm,dynamics,elevation changes,geometry,registration,retreat,rock,svalbard glaciers},
  author = {Kociuba, Waldemar and Kubisz, Waldemar and Zagórski, Piotr},
