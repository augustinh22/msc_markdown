@incollection{mukherjiSurfaceReconstructionContour2008,
  langid = {english},
  title = {Surface {{Reconstruction}} from {{Contour Lines}} or {{LIDAR}} Elevations by {{Least Squared}}-Error {{Approximation}} Using {{Tensor}}-{{Product Cubic B}}-Splines},
