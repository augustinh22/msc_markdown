@article{baraldiAutomaticSpectralRuleBasedPreliminary2010,
  title = {Automatic {{Spectral}}-{{Rule}}-{{Based Preliminary Classification}} of {{Radiometrically Calibrated SPOT}}-4/-5/{{IRS}}, {{AVHRR}}/{{MSG}}, {{AATSR}}, {{IKONOS}}/{{QuickBird}}/{{OrbView}}/{{GeoEye}}, and {{DMC}}/{{SPOT}}-1/-2 {{Imagery}} -- {{Part I}}: {{System Design}} and {{Implementation}}},
