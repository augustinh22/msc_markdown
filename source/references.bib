  keywords = {science,Monitoring,Landsat,Applications,Archive,Mapping,Policy},
  author = {Wulder, Michael A. and Masek, Jeffrey G. and Cohen, Warren B. and Loveland, Thomas R. and Woodcock, Curtis E.},
  file = {C:\\Users\\b1041827\\Zotero\\storage\\EMKWU53J\\Wulder et al. - 2012 - Opening the archive How free data has enabled the.pdf;C:\\Users\\b1041827\\Zotero\\storage\\52JBKQ36\\S003442571200034X.html}
}

@incollection{osullivanChangingLandscapeGeospatial2018,
